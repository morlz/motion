{
	"name": "@morlz/motion",
	"version": "0.0.4",
	"main": "./dist/motion.min.umd.js",
	"keywords": [
		"vue",
		"motion",
		"material",
		"morlz"
	],
	"files": [
		"dist/motion.min.umd.js"
	],
	"license": "MIT",
	"description": "Vue directives for transition components between routes",
	"author": "morlz <morlz@mail.ru>",
	"homepage": "https://github.com/morlz/motion",
	"scripts": {
		"dev": "parcel ./src/demo/index.html --out-dir serve",
		"start": "npm run dev",
		"build": "microbundle --entry ./src/lib/index.js --target browser --format umd --name @morlz/motion",
		"build:parcel": "parcel build ./src/lib/index.js --out-file motion.min.js",
		"demo": "npm run build && npm run dev"
	},
	"dependencies": {
		"core-js": "^2.5.7",
		"dom-css": "^2.1.0",
		"popmotion": "^8.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0-0",
		"@babel/plugin-transform-runtime": "^7.1.0",
		"@babel/preset-env": "^7.1.6",
		"@babel/runtime": "^7.1.5",
		"@vue/component-compiler-utils": "^2.3.0",
		"autoprefixer": "^9.3.1",
		"babel-plugin-module-resolver": "^3.1.1",
		"microbundle": "^0.7.0",
		"parcel-bundler": "^1.10.3",
		"postcss-import": "^12.0.1",
		"postcss-url": "^8.0.0",
		"regenerator-runtime": "^0.13.1",
		"stylus": "^0.54.5",
		"vue": "^2.5.17",
		"vue-hot-reload-api": "^2.3.1",
		"vue-router": "^3.0.1",
		"vue-template-compiler": "^2.5.17",
		"vuex": "^3.0.1"
	},
	"engines": {
		"node": ">= 10.0.0",
		"npm": ">= 6.0.0"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
